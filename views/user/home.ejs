    <!-- quick View -->
     <script>
        let currentModal = null;

        function setupQuickView() {
            const quickViewButtons = document.querySelectorAll('[data-bs-target="#quick_view_modal"]');
            quickViewButtons.forEach(button => {
                button.addEventListener('click', function(event) {
                    event.preventDefault();
                    const productId = this.getAttribute('data-product-id');
                    fetchProductDetails(productId);
                });
            });

            // Setup close button functionality
            const modal = document.getElementById('quick_view_modal');
            const closeButton = modal.querySelector('.btn-close');
            closeButton.addEventListener('click', () => {
                if (currentModal) {
                    currentModal.hide();
                }
            });
        }

        function fetchProductDetails(productId) {
            // Make an AJAX call to your server to get the product details
            fetch(`/api/product-details?id=${productId}`)
                .then(response => response.json())
                .then(product => {
                    updateQuickViewModal(product);
                })
                .catch(error => console.error('Error:', error));
        }

        function updateQuickViewModal(product) {
            const modal = document.getElementById('quick_view_modal');
            
            // Update modal content with product details
            modal.querySelector('.modal-product-img img').src = `/productImage/${product.img[0]}`;
            modal.querySelector('.modal-product-info h3').textContent = product.name;
            modal.querySelector('.product-price span').textContent = `$${product.promoPrice || product.price}`;
            if (product.promoPrice) {
                modal.querySelector('.product-price del').textContent = `$${product.price}`;
            } else {
                modal.querySelector('.product-price del').textContent = '';
            }
            
            // Update other fields as needed
            
            // Show the modal
            if (currentModal) {
                currentModal.dispose();
            }
            currentModal = new bootstrap.Modal(modal);
            currentModal.show();
        }
        
        // Call this function when the page loads
        document.addEventListener('DOMContentLoaded', setupQuickView);

        // Add event listener for when the modal is hidden
        document.getElementById('quick_view_modal').addEventListener('hidden.bs.modal', function () {
            if (currentModal) {
                currentModal.dispose();
                currentModal = null;
            }
        });
     </script>
  
